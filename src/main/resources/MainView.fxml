<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root_view" prefHeight="400.0" prefWidth="640.0" styleClass="root-node" stylesheets="@MainController.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hicmikrolab.hotelManagementSystem.controller.MainController">
  <children>
    <MenuBar fx:id="menu_bar" VBox.vgrow="NEVER">
         <menus>
            <Menu fx:id="configure_menu" mnemonicParsing="false" text="Configure">
              <items>
                <MenuItem fx:id="add_menu_item" mnemonicParsing="false" text="Add" />
                  <MenuItem fx:id="edit_menu_item" mnemonicParsing="false" text="Edit" />
              </items>
            </Menu>
         </menus>
    </MenuBar>
    <AnchorPane fx:id="control_pane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
  </children>
</VBox>
